<h1> Her ser du en oversigt over alle varer til salg: </h1>
   <table class="table">
      <thead>
         <th>Pris</th>
         <th>Kategori</th>
         <th>Beskrivelse</th>
         <th>Billede</th>
      </thead>
   <% goods.forEach(function(inventory) { %>
   <tr>
   <td> <%=inventory.pris %> </td>
   <td> <%=inventory.kategori %> </td>
   <td> <%=inventory.beskrivelse %></td>
   <td> <%=inventory.billede %> </td>
   </tr>
   <% }) %>
   </table>
</div>
